{
  "name": "frontend-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "prettier": "npx prettier -w \"./src/\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.0-next.6",
    "@angular/compiler": "^21.0.0-next.6",
    "@angular/core": "^21.0.0-next.6",
    "@angular/forms": "^21.0.0-next.6",
    "@angular/platform-browser": "^21.0.0-next.6",
    "@angular/router": "^21.0.0-next.6",
    "prettier": "3.6.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0-next.6",
    "@angular/cli": "^21.0.0-next.6",
    "@angular/compiler-cli": "^21.0.0-next.6",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.3.0",
    "eslint": "^9.35.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.40.0"
  }
}
